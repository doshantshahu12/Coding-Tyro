sub1=int(input("enter marks of subject 1: "))
sub2=int(input("enter marks of subject 2: "))
sub3=int(input("ener mtarks of subject 3: "))

if(sub1<33 or sub2<33 or sub3<33):
    print("you are fail in this exam because you got less then 33% in one of these subjects")

elif((sub1+sub2+sub3)/3 <40):
    print("you are fail in this examination because you got less then 40% in total")
else:
    print("congratulations!, you are passed in examination")
